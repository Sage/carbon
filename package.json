{
  "name": "carbon-react",
  "version": "3.2.0",
  "description": "A library of reusable React components and an interface for easily building user interfaces based on Flux.",
  "engineStrict": true,
  "engines": {
    "npm": "^5.0.0"
  },
  "scripts": {
    "set-config": "node -e \"require('./script/set-config').default()\"",
    "test": "jest --config=./jest.conf.json",
    "debug": "node --inspect ./node_modules/.bin/jest --watch --config=./jest.conf.json",
    "lint": "./node_modules/.bin/eslint",
    "precompile": "npm run set-config && rm -rf ./lib && node -e \"require('ncp').ncp('./src', './lib', { filter: new RegExp('^((?!__spec__.js|.md).)*$') }, function() { })\" && NODE_ENV=production babel --stage 0 ./src --out-dir ./lib --ignore '**/*/__spec__.js' --quiet",
    "prepublishOnly": "node check-version.js && npm install && npm run precompile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sage/carbon"
  },
  "author": "The Sage Group plc",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Sage/carbon/issues"
  },
  "homepage": "https://github.com/Sage/carbon#readme",
  "peerDependencies": {
    "flux": "^3.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-standalone": "~6.17.0",
    "carbon-factory": "3.3.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "~4.14.0",
    "flux": "^3.1.1",
    "gulp": "~3.9.0",
    "highlight.js": "~9.6.0",
    "jest": "~20.0.4",
    "moxios": "^0.4.0",
    "ncp": "~2.0.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dnd-test-backend": "~2.4.0",
    "react-dom": "^16.2.0",
    "react-highlight": "briancappello/react-highlight#react-v16-compiled",
    "react-test-renderer": "^16.2.0",
    "underscore.string": "~3.3.4",
    "xhr-mock": "git://github.com/resin-io-modules/xhr-mock.git#improvements"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "bignumber.js": "~4.0.2",
    "bowser": "~1.5.0",
    "classnames": "~2.2.3",
    "core-js": "^2.5.1",
    "create-react-class": "^15.6.0",
    "crypto-js": "~3.1.6",
    "escape-string-regexp": "~1.0.5",
    "events": "~1.1.1",
    "form-serialize": "~0.7.0",
    "highcharts": "~5.0.2",
    "i18n-js": "http://github.com/fnando/i18n-js/archive/v3.0.0.rc12.tar.gz",
    "immutable": "~3.8.1",
    "lodash": "~4.15.0",
    "marked": "~0.3.9",
    "moment": "~2.20.1",
    "prop-types": "^15.5.8",
    "react-day-picker": "~6.1.0",
    "react-dnd": "^2.4.0",
    "react-dnd-touch-backend": "0.2.7",
    "react-highcharts": "^15.0.0",
    "react-router": "^3.0.0",
    "react-transition-group": "^1.2.1",
    "superagent": "~3.8.2"
  }
}
