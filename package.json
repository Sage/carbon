    "prop-types": "^15.5.8",
    "react-day-picker": "~6.1.0",
    "react-dnd": "~2.4.0",
    "react-dnd-touch-backend": "0.2.7",
    "react-highcharts": "~11.5.0",
    "react-router": "^3.0.0",
    "react-transition-group": "~1.1.3",
    "superagent": "~2.2.0"
  },
  "jest": {
    "preset": "./node_modules/carbon-factory/jest.conf.json"
  }
