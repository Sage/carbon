{
  "name": "carbon-react",
  "version": "7.4.1",
  "description": "A library of reusable React components and an interface for easily building user interfaces based on Flux.",
  "engineStrict": true,
  "engines": {
    "npm": "^6.0.0"
  },
  "scripts": {
    "gen-colors": "node -e \"require('./script/generate-demo-colors').default();\"",
    "gen-docs": "node -e \"require('./script/generate-docs').default();\"",
    "prep-deploy": "node -e \"require('./script/deploy').default();\"",
    "start": "npm run gen-colors && npm run gen-docs && npm run webpack-dev-server",
    "build": "npm run gen-colors && npm run gen-docs && cross-env NODE_ENV=production npm run webpack && npm run prep-deploy",
    "test": "jest --config=./jest.conf.json",
    "test-update": "jest --config=./jest.conf.json --updateSnapshot",
    "test-cypress": "cypress open",
    "test-cypress-build": "cypress run --spec ./cypress/features/build/build.feature --env TAGS=@build",
    "test-cypress-sanity": "cypress run --spec ./cypress/features/sanity/sanity.feature --env TAGS=@sanity",
    "test-cypress-regression": "cypress run --spec ./cypress/features/regression/*.feature",
    "test-storybook-smoke": "./node_modules/.bin/start-storybook -p 9009 -c .storybook --ci --smoke-test",
    "debug": "node --inspect ./node_modules/jest-cli/bin/jest --watch --config=./jest.conf.json",
    "lint": "./node_modules/.bin/eslint",
    "lint-ts": "./node_modules/.bin/tslint 'src/**/*.ts'",
    "precompile": "npm run clean-lib && npm run copy-files && npm run babel",
    "prepublishOnly": "node check-version.js && npm install && npm run precompile",
    "watch": "npm run clean-lib && npm run copy-files -- --watch & npm run babel -- --watch",
    "storybook": "./node_modules/.bin/start-storybook -p 9001 -c .storybook",
    "storybook-ci": "./node_modules/.bin/start-storybook -p 9001 -c .storybook --ci",
    "storybook-cypress": "concurrently -k 'npm run storybook-ci' 'wait-on http://localhost:9001 && npm run test-cypress-build'",
    "storybook-test": "npm run storybook-ci && wait-on http://localhost:9001 && npm run test-cypress-build </dev/null &>/dev/null",
    "babel": "cross-env NODE_ENV=production babel --stage 0 ./src --config-file ./babel.config.js --out-dir ./lib --ignore '**/*/__spec__.js','**/*.spec.js','**/__definition__.js' --quiet",
    "clean-lib": "rimraf ./lib",
    "copy-files": "cpx \"src/**/!(*.js|*.md|*.snap)\" lib",
    "webpack": "node_modules/.bin/webpack",
    "webpack-dev-server": "node_modules/.bin/webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sage/carbon"
  },
  "author": "The Sage Group plc",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Sage/carbon/issues"
  },
  "homepage": "https://github.com/Sage/carbon#readme",
  "peerDependencies": {
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "i18n-js": "^3.0.0"
  },
  "devDependencies": {
    "@sambego/storybook-state": "^1.3.4",
    "@storybook/addon-a11y": "5.0.3",
    "@storybook/addon-actions": "5.0.3",
    "@storybook/addon-info": "5.0.3",
    "@storybook/addon-knobs": "5.0.3",
    "@storybook/addon-notes": "5.0.3",
    "@storybook/addon-viewport": "5.0.3",
    "@storybook/react": "5.0.3",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "babel-standalone": "~6.17.0",
    "carbon-factory": "7.4.1",
    "carbon-state-management": "0.1.6",
    "core-js": "^2.6.3",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "cypress-cucumber-preprocessor": "^1.11.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint-plugin-cypress": "^2.0.1",
    "events": "~1.1.1",
    "flux": "^3.1.1",
    "highlight.js": "~9.6.0",
    "i18n-js": "http://github.com/fnando/i18n-js/archive/v3.0.0.rc12.tar.gz",
    "jest-styled-components": "^6.3.1",
    "mockdate": "^2.0.2",
    "moxios": "^0.4.0",
    "raf": "^3.4.0",
    "react": "16.8.2",
    "react-dnd-test-backend": "~2.4.0",
    "react-dom": "16.8.2",
    "react-highlight": "briancappello/react-highlight#react-v16-compiled",
    "react-test-renderer": "16.8.2",
    "rimraf": "^2.6.3",
    "underscore.string": "~3.3.4",
    "xhr-mock": "git://github.com/resin-io-modules/xhr-mock.git#improvements"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bignumber.js": "~4.0.2",
    "bowser": "~1.5.0",
    "classnames": "~2.2.3",
    "crypto-js": "~3.1.6",
    "escape-string-regexp": "~1.0.5",
    "form-serialize": "~0.7.0",
    "highcharts": "~7.1.1",
    "immutable": "~3.8.1",
    "lodash": "^4.17.11",
    "marked": "^0.6.2",
    "moment": "~2.20.1",
    "prop-types": "^15.5.8",
    "react-day-picker": "~6.1.0",
    "react-dnd": "^2.4.0",
    "react-dnd-touch-backend": "0.2.7",
    "react-highcharts": "^15.0.0",
    "react-router": "^3.0.0",
    "react-transition-group": "^1.2.1",
    "styled-components": "^4.1.3",
    "superagent": "~3.8.2",
    "wait-on": "^3.2.0"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/step-definitions/"
  }
}
