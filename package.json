{
  "name": "carbon-react",
  "version": "106.0.1",
  "description": "A library of reusable React components for easily building user interfaces.",
  "engineStrict": true,
  "engines": {
    "npm": "^6.0.0",
    "node": ">=14.16.0"
  },
  "files": [
    "lib",
    "esm",
    "scripts/check_carbon_version.js",
    "scripts/check_rfcs.js"
  ],
  "scripts": {
    "start": "node ./scripts/check_node_version.js && start-storybook -p 9001 -s .assets -c .storybook",
    "start:debug-theme": "cross-env STORYBOOK_DEBUG_THEME=true npm run start",
    "test": "jest --config=./jest.conf.json",
    "test-update": "jest --config=./jest.conf.json --updateSnapshot",
    "test:cypress": "cypress open",
    "cypress:react": "npx cypress open-ct",
    "debug": "node --inspect ./node_modules/jest-cli/bin/jest --watch --config=./jest.conf.json",
    "format": "prettier --write './src'",
    "lint": "eslint ./src",
    "build": "node ./scripts/build.js",
    "copy-files": "node ./scripts/copy-files.js",
    "precompile": "npm run type-check && npm run clean-lib && npm run build && npm run build:types && npm run copy-files && npm run generate-proptypes",
    "prepublishOnly": "npm run precompile",
    "postinstall": "node ./scripts/check_carbon_version.js && node ./scripts/check_rfcs.js",
    "watch": "npm run clean-lib && npm run copy-files -- --watch & npm run babel -- --watch",
    "build-storybook": "build-storybook -c .storybook -s .assets",
    "start:static": "npx http-server -p 9001 ./storybook-static",
    "babel": "cross-env NODE_ENV=production babel ./src --config-file ./babel.config.js --out-dir ./lib --ignore '**/*/__spec__.js','**/*.spec.js','**/*.spec.ts','**/*.spec.tsx','**/*.test.js','**/*.d.ts' --quiet --extensions '.js','.ts','.tsx'",
    "clean-lib": "rimraf ./lib && rimraf ./esm",
    "commit": "git-cz",
    "generate-metadata": "node ./scripts/generate_metadata/index.mjs",
    "generate-proptypes": "ts-node ./scripts/generate_proptypes/index.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc -p tsconfig-build.json --emitDeclarationOnly --outDir ./lib && tsc -p tsconfig-build.json --emitDeclarationOnly --outDir ./esm"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sage/carbon"
  },
  "author": "The Sage Group plc",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Sage/carbon/issues"
  },
  "homepage": "https://carbon.sage.com",
  "peerDependencies": {
    "@sage/design-tokens": "^2.3.0",
    "draft-js": "^0.11.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-classes": "7.13.0",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@babel/types": "^7.16.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@cypress/react": "^5.12.0",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@sage/design-tokens": "^2.3.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-a11y": "^6.4.18",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-controls": "^6.4.18",
    "@storybook/addon-docs": "^6.4.18",
    "@storybook/addon-google-analytics": "^6.2.9",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-toolbars": "^6.4.18",
    "@storybook/addon-viewport": "^6.4.18",
    "@storybook/addons": "^6.4.18",
    "@storybook/components": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@storybook/theming": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/invariant": "^2.2.35",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.179",
    "@types/node": "10.12.18",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.8",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "axe-core": "^3.5.5",
    "babel-eslint": "10.0.3",
    "babel-jest": "^26.6.3",
    "browserslist": "^4.16.6",
    "chalk": "^4.1.1",
    "chromatic": "^6.0.5",
    "commitizen": "^4.2.4",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "core-js": "^3.20.3",
    "cross-env": "^5.2.0",
    "cypress": "^9.5.2",
    "cypress-axe": "^0.14.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-each": "^1.11.0",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-real-events": "^1.7.0",
    "cypress-storybook": "^0.5.1",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns-tz": "^1.2.2",
    "draft-js": "^0.11.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-no-unsanitized": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "events": "~1.1.1",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^10.5.3",
    "mockdate": "^2.0.2",
    "prettier": "^2.1.2",
    "raf": "^3.4.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "semver": "^7.3.5",
    "sprintf-js": "^1.1.2",
    "styled-components": "^4.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.2",
    "typescript-to-proptypes": "^2.2.1",
    "uuid": "^8.3.2",
    "webpack-dev-server": "^4.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "@popperjs/core": "^2.11.2",
    "@styled-system/prop-types": "^5.1.5",
    "@tippyjs/react": "^4.2.5",
    "@types/styled-system": "^5.1.11",
    "classnames": "~2.2.6",
    "crypto-js": "~3.3.0",
    "date-fns": "^2.26.0",
    "dotenv": "^10.0.0",
    "immutable": "~3.8.2",
    "invariant": "^2.2.4",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.7",
    "polished": "^4.0.5",
    "prop-types": "^15.7.2",
    "react-day-picker": "~7.4.10",
    "react-dnd": "^15.1.1",
    "react-dnd-html5-backend": "^15.1.2",
    "react-is": "^17.0.2",
    "react-transition-group": "^4.4.1",
    "styled-system": "^5.1.5",
    "wait-on": "^5.2.1"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/step-definitions/",
    "cucumberJson": {
      "generate": false,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "preset": "conventionalcommits",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "successCmd": "echo \"SEMVER_VERSION=${nextRelease.version}\" >> $GITHUB_ENV"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
