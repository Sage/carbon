{
  "baseUrl": "http://localhost:9001/",
  "blacklistHosts": ["www.google-analytics.com", "sockjs*", "countries*"],
  "env":
     {
       "TAGS": "not @ignore",
       "RETRIES": 3,
       "RETRIES_HIDDEN": 1,
       "RETRIES_NO_LOG": 1
      },
  "failOnStatusCode": false,
  "ignoreTestFiles": ["**/examples/*", "**/!(*.spec).js"],
  "integrationFolder": "cypress/features",
  "numTestsKeptInMemory": 1,
  "video": false,
  "viewportWidth": 1366,
  "viewportHeight": 768,
  "reporter": "mocha-allure-reporter, spec",
  "chromeWebSecurity": false
}
